CREATE TABLE Position(
    id int GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    position_name varchar(100) NOT NULL UNIQUE

);

CREATE TABLE Employee(
    id int GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    employee_name varchar(100) NOT NULL,
    position_id int REFERENCES Position(id) ON DELETE SET NULL
);

CREATE TABLE Project(
			id int ,
                         project_name varchar(100) NOT NULL,
                         employee_id int REFERENCES Employee(id) ON DELETE SET NULL,
                         customer_id int REFERENCES Customer(id) ON DELETE SET NULL
);

insert into position values (1, 'Safety');
insert into position values (2, 'Programmer');
insert into position values (3, 'Builder');
insert into position values (4, 'Driver');

insert into employee values (1,'Alex',1);
insert into employee values (2,'Viktor',2);
insert into employee values (3,'Elena',2);
insert into employee values (4,'Petya',3);

